li {
    line-height: 2em;
    font-size: 16px
}

td {
    max-width: 100px;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

th:hover {
    background-color: gainsboro;
}

.function-cell {
    background-color: antiquewhite;
}

.error-cell {
    background-color: orangered;
}